{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "email": {
        "type": "string",
        "pattern": "[a-zA-Z0-9._%+-]+@gmail\\.com$"
      },
      "password": {
        "type": "string",
        "pattern": "^(0[1-9]|1[0-2])\/[0-9]{2}$"
      }
    },
    "required": ["email", "password"]
}
